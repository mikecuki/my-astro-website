    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "prettier-plugin-astro": "^0.11.0"
  }
}
